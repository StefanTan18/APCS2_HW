import java.util.ArrayList;

public class ALQueue<T>
{
    ArrayList<T> x = new ArrayList<T>();
    public void enqueue(T item){
	x.add(item);
    }
    public T dequeue(){
	return x.remove(0);
    }
    public boolean isEmpty(){
	return (x.size() == 0);
    }

    public T peekFront(){
	return x.get(0);
    }
    public static void main(String[] args){
	System.out.println( "isEmpty: " + aQueue.isEmpty() ); //false
	//aoo
	System.out.println( "peek: " + aQueue.peekFront() );
	System.out.println( "dequeue: " + aQueue.dequeue() );
	//boo
	System.out.println( "peek: " + aQueue.peekFront() );
	System.out.println( "dequeue: " + aQueue.dequeue() );
	//coo
	System.out.println( "peek: " + aQueue.peekFront() );
	System.out.println( "dequeue: " + aQueue.dequeue() );
	//doo
	System.out.println( "peek: " + aQueue.peekFront() );
	System.out.println( "dequeue: " + aQueue.dequeue() );
	System.out.println( "isEmpty: " + aQueue.isEmpty() ); //tru
    }
}
