public class Resrever{
    public static String reverse(String s){
	String retStr = "";
	for (int i = s.length() - 1; i >= 0; i--)
	    retStr += s.substring(i, i+1);
	return retStr;
    }

    public static void main(String[] args){
	String s1 = "stressed";
	System.out.println(reverse(s1));
    }
}
      
	    
	
