public class LList implements List{
    private LLNode head;

    public boolean add(String x){
	head.setCargo(x)
	return true;
    }

    public String get(int i){
	if(i = 0){
	    return head.getCargo();
	}
	else{
	    i--;
	    return get(head.getNext());
	}
    }

    public String set(int i, String x){
	while(i > 0){
	    head.getNext();
	    i--;
	}
	head.setCargo(x);
    }
}
	       
	    
	
	    
			      
